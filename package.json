{
  "name": "idai-field-client",
  "description": "iDAI.field 2 Client",
  "main": "main.js",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild" : "rimraf dist/*",
    "build": "gulp build",
    "start": "npm run build && gulp run",
    "test-loop": "karma start dist/test/karma.conf",
    "pretest": "npm run build",
    "test": "karma start dist/test/karma.conf.js --single-run",

    "e2e": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/protractor dist/test/protractor.conf.js",
    "server": "gulp webserver-watch",
    "package": "rimraf release/* && gulp package",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codarchlab/idai-field-client.git"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "angular2-uuid": "^1.0.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "^0.19.26",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "archiver": "^1.0.0",
    "bootstrap-sass": "^3.3.6",
    "electron-connect": "^0.3.4",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^0.37.2",
    "gulp": "^3.9.0",
    "gulp-angular-embed-templates": "^2.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.3",
    "gulp-sass": "^2.1.1",
    "gulp-typescript": "^2.10.0",
    "gulp-uglify": "^1.5.1",
    "gulp-webserver": "^0.9.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "mdi": "^1.5.54",
    "protractor": "^3.1.1",
    "rimraf": "^2.5.2",
    "typescript": "^1.8.10"
  }
}
