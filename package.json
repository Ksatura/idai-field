{
  "name": "idai-field-client",
  "description": "iDAI.field 2 Client",
  "main": "main.js",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf css/* && rimraf src/* && rimraf fonts/*",
    "build": "gulp create-configs && gulp compile",
    "start": "gulp run",
    "test:loop": "karma start test/browser/config/karma.conf",
    "test": "jasmine JASMINE_CONFIG_PATH=test/node/config/jasmine.json && karma start test/browser/config/karma.conf.js --single-run",
    "pree2e": "rimraf test/e2e-screenshots/*.png",
    "e2e": "protractor test/e2e/config/protractor.conf.js",
    "server": "gulp webserver-watch",
    "premake-dist": "rimraf dist/*",
    "make-dist": "gulp make-dist && gulp versioning --build $1",
    "prepackage": "rimraf dist/mediastore && rimraf release/*",
    "package": "gulp package && electron-packager ./dist --platform=darwin --arch=x64 --icon=dist/img/logo --prune=true --out=release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codarchlab/idai-field-client.git"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.0.0-rc.3",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.14",
    "angular2-uuid": "1.1.0",
    "es6-shim": "0.35.0",
    "idai-components-2": "git+http://bogusman03.dai-cloud.uni-koeln.de/idai-components-2-builds/394/.git",
    "identicon.js": "github:dainst/identicon.js",
    "leaflet": "1.0.1",
    "leaflet-imageoverlay-rotated": "0.1.1",
    "leaflet.pm": "0.13.0",
    "mdbootstrap": "4.2.0",
    "mdi": "1.7.22",
    "papaparse": "github:dainst/PapaParse",
    "pouchdb": "^6.1.1",
    "reflect-metadata": "0.1.8",
    "roboto-fontface": "0.6.0",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.31",
    "ts-md5": "1.2.0",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@types/electron": "^1.4.31",
    "@types/es6-shim": "^0.31.32",
    "@types/geojson": "0.0.32",
    "@types/jasmine": "^2.5.41",
    "@types/leaflet": "^1.0.49",
    "@types/node": "7.0.3",
    "@types/papaparse": "^4.1.28",
    "@types/pouchdb": "^5.4.28",
    "archiver": "1.1.0",
    "chromedriver": "2.25.1",
    "electron-connect": "0.4.2",
    "electron-packager": "8.4.0",
    "electron-prebuilt": "1.4.13",
    "electron-rebuild": "1.5.7",
    "file-api": "0.10.4",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-replace": "0.5.4",
    "gulp-sass": "2.3.2",
    "gulp-typescript": "3.1.4",
    "gulp-uglify": "1.5.1",
    "gulp-webserver": "0.9.1",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "protractor": "4.0.10",
    "protractor-console-plugin": "0.1.1",
    "rimraf": "2.5.4",
    "typescript": "2.1.5",
    "yargs": "5.0.0"
  }
}
