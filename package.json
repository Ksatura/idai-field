{
  "name": "idai-field-client",
  "description": "iDAI.field 2 Client",
  "main": "main.js",
  "version": "0.2.0",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf css/* && rimraf src/* && rimraf fonts/*",
    "build": "gulp create-configs && gulp compile",
    "start": "gulp run",
    "test:loop": "karma start test/browser/config/karma.conf",
    "test": "jasmine JASMINE_CONFIG_PATH=test/node/config/jasmine.json && karma start test/browser/config/karma.conf.js --single-run",
    "e2e": "protractor test/e2e/config/protractor.conf.js",
    "server": "gulp webserver-watch",
    "make-dist": "rimraf dist/* && gulp make-dist && gulp versioning --build $1",
    "postinstall": "typings install",
    "package": "rimraf release/* && gulp package && electron-packager ./dist --platform=darwin --arch=x64 --icon=dist/img/logo --prune=true --out=release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codarchlab/idai-field-client.git"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.0.0-rc.3",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.14",
    "angular2-uuid": "^1.1.0",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "idai-components-2": "git+http://bogusman03.dai-cloud.uni-koeln.de/idai-components-2-builds/351/.git",
    "identicon.js": "github:dainst/identicon.js",
    "leaflet": "1.0.1",
    "leaflet-imageoverlay-rotated": "^0.1.1",
    "leaflet.pm": "^0.10.1",
    "mdbootstrap": "4.2.0",
    "mdi": "1.7.22",
    "papaparse": "github:dainst/PapaParse",
    "reflect-metadata": "^0.1.8",
    "roboto-fontface": "^0.6.0",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.31",
    "ts-md5": "^1.2.0",
    "zone.js": "0.6.21"
  },
  "devDependencies": {
    "@types/node": "^6.0.45",
    "archiver": "^1.1.0",
    "chromedriver": "2.25.1",
    "electron-connect": "0.4.2",
    "electron-packager": "8.4.0",
    "electron-prebuilt": "1.4.13",
    "electron-rebuild": "^1.5.7",
    "file-api": "^0.10.4",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.3.2",
    "gulp-typescript": "^2.10.0",
    "gulp-uglify": "^1.5.1",
    "gulp-webserver": "^0.9.1",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "karma": "1.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "protractor": "4.0.10",
    "protractor-console-plugin": "^0.1.1",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "yargs": "^5.0.0"
  }
}
