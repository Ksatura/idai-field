{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Field 3 API file listing",
  "type": "object",
  "patternProperties": {
    ".+": {
      "type": "object",
      "required": [
        "deleted",
        "types",
        "file_info"
      ],
      "additionalProperties": false,
      "properties": {
        "deleted": {
          "type": "boolean"
        },
        "types": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "enum": [
              "original_image",
              "thumbnail_image"
            ]
          }
        },
        "file_info": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "required": [
              "size",
              "variant"
            ],
            "additionalProperties": false,
            "properties": {
              "size": {
                "type": "number"
              },
              "variant": {
                "type": "string",
                "enum": [
                  "original_image",
                  "thumbnail_image"
                ]
              }
            }
          }
        }
      }
    }
  }
}