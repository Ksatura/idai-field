{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "iDAI.field 3 API file listing",
  "type": "object",
  "patternProperties": {
    ".+": { 
      "type": "object",
      "required": ["deleted", "types"],
      "properties": {
        "deleted": {
          "type": "boolean"
        },
        "types": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "string",
            "enum": ["original_image", "thumbnail_image"]
          }
        }
      } 
    }
  }
}